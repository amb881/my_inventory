from item import Item

class Client_Hardware(Item):
    def __init__(self, comp_num, type, sub_type, model, serial_number, operating_system, supplier, details):
        self.Client_Hardware_comp_num = comp_num
        self.Client_Hardware_type = type
        self.Client_Hardware_sub_sytpe = sub_type
        self.Client_Hardware_model = model
        self.Client_Hardware_serial_number = serial_number
        self.Client_Hardware_operating_system = operating_system
        self.Client_Hardware_supplier = supplier
        self.Client_Hardware_details = details
